<h1>Random Beer</h1>
<div class="random-beer-container">
    <div>
        {{#each randomBeer}}
        {{> beerpartial }}
        <p><strong>Food Pairing: </strong>
        <ul class="food-pairing-list">  
            {{#each food_pairing}}
